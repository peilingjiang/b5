    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-scss": "^3.0.4",
    "precss": "^4.0.0",
    "prettier": "^2.2.1",
    "resize-observer-polyfill": "^1.5.1"
  },
  "type": "module",
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,css}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,html,css}": "prettier --write"
  }
