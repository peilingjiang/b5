{
  "name": "b5",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "blob-polyfill": "^5.0.20210201",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.4.6",
    "p5": "^1.2.0",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-device-detect": "^1.15.0",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
